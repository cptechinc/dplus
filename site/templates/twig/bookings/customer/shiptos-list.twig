<table class="table table-sm">
	<thead>
		<tr>
			<th>Date</th>
			<th>Amount</th>
		</tr>
	</thead>
	<tbody>
		{% for shipto in shiptos %}
		<tr>
			<td>
				<a href="{{ page.get_ci_customerURL(shipto.custid, shipto.shiptoid) }}" class="link">
					{{ shipto.shipto_name() }} ({{ shipto.shiptoid }})
				</a>
			</td>
			<td>$ {{ shipto.amount|currency }}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
