<h4>Inspection</h4>
<table class="table table-striped table-sm">
	<thead>
		{% for column in json['columns']['inspection notes'] %}
			<th class="{{ module_json.get_justifyclass(column['headingjustify']) }}">
				{{ column['heading'] }}
			</th>
		{% endfor %}
	</thead>
	<tbody>
		{% for note in json['data']['inspection notes'] %}
			<tr>
				{% for column, properties in json['columns']['inspection notes'] %}
					<td class="{{ module_json.get_justifyclass(properties['datajustify']) }}">
						{{ note[column] }}
					</td>
				{% endfor %}
			</tr>
		{% endfor %}
	</tbody>
</table>

