<div class="card">
	<div class="card-header">
		<div class="row">
			<div class="col-sm-3">
				<strong>Mnfr / Comp ID:</strong> {{ vendor.vendorid }}
			</div>
			<div class="col-sm-6">
				<div>
					<small>{{ vendor.name }}</small><br>
					<small></small>
				</div>
			</div>
		</div>
	</div>

	{% include 'items/mxrfe/list/vendor/list.twig' with {'page': page, 'mxrfe': mxrfe, 'xrefs': xrefs} %}

	{% include 'util/paginator.twig' with {'page': page, 'resultscount': xrefs.getNbResults()} %}

	<div class="card-footer">
		<div class="d-flex justify-content-between align-items-center">
			<div></div>
			<div>
				<a href="{{ page.url }}" class="btn btn-warning btn-sm">
					<i class="fa fa-times" aria-hidden="true"></i> Close
				</a>
			</div>
		</div>
	</div>
</div>
