<nav aria-label="breadcrumb rounded-0">
	<ol class="breadcrumb">
		{% for parent in page.parents() %}
			<li class="breadcrumb-item">
				<i class="fa fa-list" aria-hidden="true"></i>
				<a href="{{ parent.url }}">{{ parent.title }}</a>
			</li>
		{% endfor %}
		{% if page.has('itemID') %}
			<li class="breadcrumb-item">
				<a href="{{ page.url }}">{{ page.title }}</a>
			</li>
			<li class="breadcrumb-item active" aria-current="page">Item: {{ page.itemID }}</li>
		{% endif %}

		{% if upc %}
			<li class="breadcrumb-item">
				<a href="{{ page.url }}">{{ page.title }}</a>
			</li>
			<li class="breadcrumb-item">
				<a href="{{ page.item_upcsURL(upc.itemid) }}">Item: {{ upc.itemid }}</a>
			</li>
			<li class="breadcrumb-item active" aria-current="page">UPC: {{ upc.upc }}</li>
		{% endif %}
	</ol>
</nav>
