<div class="list-group">
	{% for item in items %}
		<a href="{{ page.get_itemURL(item.itemid) }}" class="list-group-item list-group-item-action item-link" data-itemid="{{ item.itemid }}">
			<h4 class="list-group-item-heading">{{ item.itemid }}</h4>
			<p class="list-group-item-text mb-0">{{ item.desc }}</p>
		</a>
	{% endfor %}
</div>
