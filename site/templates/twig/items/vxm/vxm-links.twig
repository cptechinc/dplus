<div class="mb-3">
	<button class="btn btn-primary" onclick="window.history.back()">
		<i class="fa fa-arrow-left" aria-hidden="true"></i> Back
	</button>
	&nbsp;
	<a href="{{ page.url }}" class="btn btn-primary">
		<i class="fa fa-search" aria-hidden="true"></i> Search Item ID or Vendor ID
	</a>
	
	{% if page.has('vendorID') and page.has('vendoritemID') == false %}
		<a href="{{ page.vxm_vendors_itemURL(page.vendorID, 'new') }}" class="btn btn-primary">
			<i class="fa fa-plus" aria-hidden="true"></i> Add Item to Vendor {{ page.vendorID }}
		</a>
	{% endif %}

	{% if page.has('vendoritemID') %}
		<a href="{{ page.vxm_vendorURL(page.vendorID) }}" class="btn btn-primary"> View Vendor {{ page.vendorID }}</a>
	{% endif %}
</div>
