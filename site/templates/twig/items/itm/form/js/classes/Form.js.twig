class ItmForm {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new ItmForm();
		}
		return this.instance;
	}

	constructor() {
		this.id = 'itm-form';
		this.form = $('#' + this.id);
		this.inputs = ItmInputs.getInstance();
		this.config = ItmConfigs.getInstance();
	}
}
