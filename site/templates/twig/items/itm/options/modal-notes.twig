<!-- Modal -->
<div class="modal fade" id="edit-note-modal" tabindex="-1" role="dialog" aria-labelledby="edit-note-modal-title" aria-hidden="true">
	<div class="modal-dialog modal-lg" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="edit-note-modal-title">Modal title</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">
				<div class="bg-light p-2 mb-3">
					<div class="row">
						<div class="col-sm-6">
							<b>Type: </b> <span class="notetype"></span>
						</div>
						<div class="col-sm-6">
							<b>Description: </b> <span class="sysopdesc"></span>
						</div>
					</div>
				</div>
				{% block form %}
					{% include 'items/itm/options/modal/notes/form.twig' %}
				{% endblock %}
			</div>
		</div>
	</div>
</div>
