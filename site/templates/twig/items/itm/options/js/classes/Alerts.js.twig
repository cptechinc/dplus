class CodeAlerts extends Alerts {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new CodeAlerts();
		}
		return this.instance;
	}

	missingRequired() {
		swal2.fire({
			title: 'Missing Required',
			text: 'Required option not entered',
			icon: 'error',
			confirmButtonText: 'Ok',
		});
	}
}
