class CostingForm {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new CostingForm();
		}
		return this.instance;
	}

	constructor() {
		this.id = 'item-costing-form';
		this.form = $('#' + this.id);
		this.inputs = CostingInputs.getInstance();
	}
}
