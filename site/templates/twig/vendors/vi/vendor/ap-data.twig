<div class="card">
	<div class="card-header">
		<span class="h5">AP</span>
	</div>
	<ul class="list-group list-group-flush">
		<li class="list-group-item">
			<div class="row">
				<div class="col-sm-10">
					Our account:
				</div>
				<div class="col-sm-2">
					{{ vendor.vendor_account }}
				</div>
			</div>
		</li>
		<li class="list-group-item">
			<div class="row">
				<div class="col-sm-10">
					Buyer:
				</div>
				<div class="col-sm-2">
				</div>
			</div>
		</li>
		<li class="list-group-item">
			<div class="row">
				<div class="col-sm-10">
					Terms:
				</div>
				<div class="col-sm-2">
					{{ vendor.termscode }}
				</div>
			</div>
		</li>
		<li class="list-group-item">
			<div class="row">
				<div class="col-sm-10">
					Type Code:
				</div>
				<div class="col-sm-2">
					{{ vendor.typecode }}
				</div>
			</div>
		</li>
	</ul>
</div>
