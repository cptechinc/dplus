<table class="table table-borderless table-striped table-sm">
    <thead>
        {% for column in json['columns']['vendor costing'] %}
            <th>{{ column['heading'] }}</th>
        {% endfor %}
    </thead>
    <tbody>
        {% for i in 1..json['data']['vendor costing']|length %}
            <tr>
                {% for cost in json['data']['vendor costing'][i] %}
                    <td>{{ cost }}</td>
                {% endfor %}
            </tr>
        {% endfor %}
    </tbody>
</table>

