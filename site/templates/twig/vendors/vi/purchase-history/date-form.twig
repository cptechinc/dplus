<form action="{{ input.url() }}" method="GET">
	<input type="hidden" name="vendorID" value="{{ input.get.text('vendorID') }}">

	<div class="form-group">
		<label for="date">Starting Report Date</label>
		{% include 'util/date-picker.twig' with {'name': 'date', 'value': '', 'id': 'date'} %}
	</div>
	<button type="submit" class="btn btn-success">Submit</button>
</form>
