<table class="table table-striped" id="users-table">
	<thead>
		<tr class="bg-dark text-white">
			<th>Login ID</th>
			<th>Name</th>
			<th>Whse</th>
		</tr>
	</thead>
	{% for user in results %}
		<tr>
			<td>
				<a href="#" class="link user-link text-monospace" data-userid="{{ user.userid }}">
					{{ user.userid|replace({' ': '&nbsp;'})|raw }}
				</a>
			</td>
			<td>{{ user.name }}</td>
			<td>{{ user.whseid }}</td>
		</tr>
	{% endfor %}
</table>
