class IarnInputs {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new IarnInputs();
		}
		return this.instance;
	}

	constructor() {
		this.id            = 'iarn-form',
		this.form          = $('#' + this.id),
		this.code          = this.form.find('input[name=id]');
		this.description   = this.form.find('input[name=description]');
	}
}
