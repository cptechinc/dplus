class IarnConfig {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new IarnConfig();
		}
		return this.instance;
	}

	constructor() {
		this.inputs = {
			id: {
				maxlength: {{ iarn.fieldAttribute('id', 'maxlength') }}
			},
			description: {
				maxlength: {{ iarn.fieldAttribute('description', 'maxlength') }}
			}
		}
	}
}
