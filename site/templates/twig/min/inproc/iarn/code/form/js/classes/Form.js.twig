class IarnForm {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new IarnForm();
		}
		return this.instance;
	}

	constructor() {
		this.id = 'iarn-form';
		this.inputs = IarnInputs.getInstance();
		this.config = IarnConfig.getInstance();
	}

	redirectExisting(goEdit) {
		if (goEdit === true) {
			var uri = URI();
			uri.setQuery('id', this.inputs.code.val());
			window.location.href = uri.toString();
		} else {
			location.reload();
		}
	}
}
