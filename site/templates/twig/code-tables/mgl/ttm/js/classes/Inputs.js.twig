class TtmInputs {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new TtmInputs();
		}
		return this.instance;
	}

	constructor() {
		this.id            = 'code-form',
		this.form          = $('#' + this.id),
		this.code          = this.form.find('input[name=code]');
		this.text1         = this.form.find('input[name=text1]');
		this.text2         = this.form.find('input[name=text2]');
		this.text3         = this.form.find('input[name=text3]');
		this.text4         = this.form.find('input[name=text4]');
	}
}
