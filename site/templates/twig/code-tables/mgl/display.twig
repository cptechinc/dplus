{% set configGl = page.wire('modules').get('Dconfigs').config('gl') %}

<div class="mb-3">
	{% if page.is_print() %}
		<button onclick="window.history.back();" class="btn btn-primary d-print-none">
			<i class="fa fa-arrow-left" aria-hidden="true"></i> Back
		</button>
		&nbsp;
		<button class="btn btn-primary d-print-none" onclick="window.print();return false;">
			<i class="fa fa-print" aria-hidden="true"></i> Print
		</button>
	{% else %}
		<a href="{{ page.menuUrl() }}" class="btn btn-primary">
			<i class="fa fa-arrow-left" aria-hidden="true"></i> Back to GLMAIN
		</a>
		&nbsp;
		<a href="{{ page.get_printURL() }}" class="btn btn-primary">
			<i class="fa fa-print" aria-hidden="true"></i> View Printable
		</a>
	{% endif %}
</div>

<div class="card mb-5">
	{% block header %}
		<div class="card-header">
			<div class="d-flex w-100 justify-content-between">
				<div></div>
				<div></div>
				<div>
					<b>Co:</b> {{ configGl.companyid }}
				</div>
			</div>
		</div>
	{% endblock %}

	{% block body %}
	{% endblock %}

	{% block footer %}
		<div class="card-footer">
			<div class="d-flex justify-content-between align-items-center">

			</div>
		</div>
	{% endblock %}
</div>
