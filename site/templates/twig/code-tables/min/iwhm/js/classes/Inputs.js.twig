class IwhmInputs {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new IwhmInputs();
		}
		return this.instance;
	}

	constructor() {
		this.id = 'code-edit-form',
		this.form = $('#' + this.id),
		this.whseid      = this.form.find('input[name=whseID]'),
		this.name        = this.form.find('input[name=name]'),
		this.pickdetail  = this.form.find('input[name=pickdetail]'),
		this.consignment = this.form.find('input[name=consignment]'),
		this.custid      = this.form.find('input[name=custID]'),
		this.cashname    = this.form.find('input[name=cash_name]'),
		this.warehouses  = {
			profit: this.form.find('select[name=whse_profit]'),
			supply: this.form.find('select[name=whse_supply]'),
			asset: this.form.find('select[name=whse_asset]'),
		},
	}
}
