class IwhmRequests {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new IwhmRequests();
		}
		return this.instance;
	}

	getCustomer(custID, callback) {
		var ajax = new AjaxRequest("{{ page.jsonApiUrl('mar/customer/') }}");
		ajax.setData({custID: custID});
		ajax.request(function(response) {
			callback(response)
		});
	}

	validateWhseid(whseID, callback) {
		var ajax = new AjaxRequest("{{ page.jsonApiUrl('inv/validate/warehouse/') }}");
		ajax.setData({whseID: whseID});
		ajax.request(function(response) {
			callback(response)
		});
	}
}
