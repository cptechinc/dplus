<h3>Notes</h3>
<div class="list-group">
	<div class="list-group-item list-group-item-action bg-dark text-white">
		<div class="row">
			<div class="col-sm-2">Type</div>
			<div class="col-sm-10">Note</div>
		</div>
	</div>
	{% for note in notes %}
		<div class="list-group-item">
			<div class="row">
				<div class="col-sm-2">{{ note.type }}</div>
				<div class="col-sm-10">
					{{ note.note }} <br>

					{% for subnote in note.get_othernotelines() %}
						{{ subnote.note }} <br>
					{% endfor %}
				</div>
			</div>
		</div>
	{% endfor %}
</div>
