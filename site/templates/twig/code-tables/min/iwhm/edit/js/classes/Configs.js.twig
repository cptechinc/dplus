class IwhmConfigs {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new IwhmConfigs();
		}
		return this.instance;
	}

	constructor() {
		this.fields = {
			code: {
				maxlength: {{ iwhm.fieldAttribute('code', 'maxlength') }}
			},
			name: {
				maxlength: {{ iwhm.fieldAttribute('name', 'maxlength') }}
			},
		};
	}
}
