class MsdsmConfigs {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new MsdsmConfigs();
		}
		return this.instance;
	}

	constructor() {
		this.fields = {
			code: {
				maxlength: {{ msdsm.fieldAttribute('code', 'maxlength') }}
			},
			description: {
				maxlength: {{ msdsm.fieldAttribute('description', 'maxlength') }}
			},
			effectivedate: {
				dateformat: '{{ msdsm.fieldAttribute('effectivedate', 'dateformat') }}',
				momentformat: "MM/DD/Y"
			}
		};
	}
}
