class SpmForm {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new SpmForm();
		}
		return this.instance;
	}

	constructor() {
		this.id = 'spm-form';
		this.form = $('#' + this.id);
		this.inputs = SpmInputs.getInstance();
		this.config = SpmConfigs.getInstance();
	}
}
