class SpmInputs {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new SpmInputs();
		}
		return this.instance;
	}

	constructor() {
		this.id     = 'code-form',
		this.form    = $('#' + this.id),
		this.repid   = this.form.find('input[name=id]');
		this.name    = this.form.find('input[name=name]');
	}
}
