{% set value = code.eom_from_day(x) %}
{% set tabindex = startindex + (x * 10) + 0 %}

{% include 'util/bootstrap/form/input.twig' with {
	'input': {
		'name': 'eom_from_day' ~ x,
		'type': 'number',
		'value': value ? value : '',
		'size': 'sm', 
		'addclasses': [
			'text-right',
			'eom_from_day'
		],
		'attributes': {
			'tabindex': '-1',
			'readonly': true,
			'max': manager.fieldAttribute('eom_from_day', 'max')
		}
	}
} %}


