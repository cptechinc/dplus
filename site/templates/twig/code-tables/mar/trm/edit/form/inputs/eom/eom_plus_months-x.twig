{% set value = code.eom_plus_months(x) %}
{% set tabindex = startindex + (x * 10) + 6 %}

{% include 'util/bootstrap/form/input.twig' with {
	'input': {
		'name': 'eom_plus_months' ~ x,
		'type': 'number',
		'value': value > 0 ? value : '',
		'size': 'sm', 
		'addclasses': [
			'text-right',
			'eom_plus_months'
		],
		'attributes': {
			'tabindex': tabindex,
			'data-error': '.error',
			'readonly': code.eom_thru_day(x) == 0,
			'max': manager.fieldAttribute('eom_plus_months', 'max'),
		}
	}
} %}