class CodeConfigs {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new CodeConfigs();
		}
		return this.instance;
	}

	constructor() {
		this.fields = {
			code: {
				maxlength: {{ manager.fieldAttribute('code', 'maxlength') }}
			},
			description: {
				maxlength: {{ manager.fieldAttribute('description', 'maxlength') }}
			},
			servicerate: {
				precision: {{ manager.fieldAttribute('servicerate', 'precision') }}
			},
			transactioncost: {
				precision: {{ manager.fieldAttribute('transactioncost', 'precision') }}
			},
			cc_surcharge_percent: {
				precision: {{ manager.fieldAttribute('cc_surcharge_percent', 'precision') }}
			},
		};
	}
}
