<div class="row">
	<div class="col-sm-3 mb-2">
		<label for="service">UPS Service</label>
	</div>
	<div class="col-sm-4 mb-2">

		{% include 'util/bootstrap/form/select-simple.twig' with {
				'select': {
					'name': 'service',
					'size': 'sm',
					'value' : manager.fieldAttribute('service', 'default'),
					'options': manager.fieldAttribute('service', 'options')
				}
			}
		%}
	</div>
</div>

<div class="row">
	<div class="col-sm-3 mb-2">
		<label for="billing">UPS Billing</label>
	</div>
	<div class="col-sm-4 mb-2">
		{% include 'util/bootstrap/form/select-simple.twig' with {
				'select': {
					'name': 'billing',
					'size': 'sm',
					'value' : manager.fieldAttribute('billing', 'default'),
					'haskeys': true,
					'showvalue': true,
					'options': manager.fieldAttribute('billing', 'options')
				}
			}
		%}
	</div>
</div>

<div class="row">
	<div class="col-sm-3 mb-2">
		<label for="residential">UPS Residential</label>
	</div>
	<div class="col-sm-4 mb-2">
		{% include 'util/bootstrap/form/select-simple.twig' with {
				'select': {
					'name': 'residential',
					'size': 'sm',
					'value' : manager.fieldAttribute('residential', 'default'),
					'haskeys': true,
					'showvalue': true,
					'options': manager.fieldAttribute('residential', 'options')
				}
			}
		%}
	</div>
</div>