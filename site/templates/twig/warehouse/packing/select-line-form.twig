<div class="form-group">
	<a href="" class="btn btn-danger exit-order">Exit Order</a>
</div>
<div class="row">
	<div class="col-sm-12">
		<div class="mb-5">
			<h3>Order Lines</h3>
			<div class="list-group">
				{% for line in lines %}
					<a href="" class="list-group-item">
						<div class="row">
							<div class="col-sm-6">
								<h4 class="list-group-item-heading">{{ line.itemid }}</h4>
							</div>
							<div class="col-sm-3">
								To Ship: {{ warehousepacking.get_line_toship(line.linenbr) }}
