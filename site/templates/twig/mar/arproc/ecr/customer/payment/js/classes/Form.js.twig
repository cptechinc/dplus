class EcrForm {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new EcrForm();
		}
		return this.instance;
	}

	constructor() {
		this.id = 'payment-form';
		this.form = $('#' + this.id);
		this.inputs = EcrInputs.getInstance();
		this.config = EcrConfigs.getInstance();
	}
}
