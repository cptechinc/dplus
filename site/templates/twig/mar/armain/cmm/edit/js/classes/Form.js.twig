class CmmForm {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new CmmForm();
		}
		return this.instance;
	}

	constructor() {
		this.id = 'cmm-form';
		this.form = $('#' + this.id);
		this.inputs = CmmInputs.getInstance();
		this.config = CmmConfigs.getInstance();
	}
}
