class CmmInputs {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new CmmInputs();
		}
		return this.instance;
	}

	constructor() {
		this.id      = 'cmm-form',
		this.form    = $('#' + this.id),
		this.custid  = this.form.find('input[name=id]');
		this.name    = this.form.find('input[name=name]');
	}

}
