<div class="card mb-5">
	{% block header %}
		<div class="card-header">
			<div class="row">
				<div class="col-sm-2">
					<strong>Cust ID:</strong>
				</div>
				<div class="col-sm-4">
					{{ customer.id  }}
				</div>
				<div class="col-sm-6">
					{{ customer.name }}
				</div>
			</div>
		</div>
	{% endblock %}

	{% block body %}
		<div class="card-body">
			{% include 'mar/armain/cmm/edit/form.twig' %}
		</div>
	{% endblock %}

	{% block footer %}
		<div class="card-footer">
			<div class="d-flex justify-content-between align-items-center">

			</div>
		</div>
	{% endblock %}
</div>
