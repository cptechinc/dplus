{% for id, warehouse in json['data'] %}
	<table class="table table-striped table-bordered table-sm table-excel" id="{{ whse['Whse Name'] }}">
		<thead></thead>
		<tbody>
			{% for id, quote in warehouse['quotes'] %}
				{% for x in 1..blueprint['header']['rowcount']  %}
					<tr class="bg-dark">
						{% set columncount = 0 %}
						{% for i in 1..blueprint['cols'] + 1 %}
							{% set colspan = 0 %}
							{% if blueprint['header']['rows'][x]['columns'][i] %}
								{% set column = blueprint['header']['rows'][x]['columns'][i] %}
								{% set colspan = column['col-length'] %}
								<td colspan="{{ colspan }}" class="{{ module_formatter.get_justifyclass(column['data-justify']) }} text-white">
									<span class="font-weight-bold">{{ column['label'] }}</span>: {{ module_formatter.format_celldata(quote, column) }} &nbsp;
									{% if i == 1 and quote['Quote ID'] %}
										{% if docm.count_documents(quote['Quote ID']) %}
											<a href="{{ page.documentsview_quote(custID, quote['Quote ID'])}}" class="text-success" title="View Documents for Quote # {{ quote['Quote ID'] }}">
												<i class="fa fa-file-text" aria-hidden="true"></i> <span class="sr-only">View Documents for Quote # {{ quote['Quote ID'] }}</span>
											</a>
										{% endif %}
									{% endif %}
								</td>
							{% else %}
								{% if columncount < blueprint['cols'] %}
									{% set colspan = 1 %}
									<td ></td>
								{% endif %}
							{% endif %}
							{% set columncount = columncount + colspan %}
						{% endfor %}
					</tr>
				{% endfor %}
				{% for x in 1..blueprint['details']['rowcount']  %}
					<tr>
						{% set columncount = 0 %}
						{% for i in 1..blueprint['cols'] + 1 %}
							{% set colspan = 0 %}
							{% if blueprint['details']['rows'][x]['columns'][i] %}
								{% set column = blueprint['details']['rows'][x]['columns'][i] %}
								{% set colspan = column['col-length'] %}
								<th colspan="{{ colspan }}" class="{{ module_formatter.get_justifyclass(column['headingjustify']) }}">
									{{ column['label'] }}
								</th>
							{% else %}
								{% if columncount < blueprint['cols'] %}
									{% set colspan = 1 %}
									<th></th>
								{% endif %}
							{% endif %}
							{% set columncount = columncount + colspan %}
						{% endfor %}
					</tr>
				{% endfor %}
				{% for linenbr, line in quote['details'] %}
					{% for x in 1..blueprint['details']['rowcount']  %}
						<tr>
							{% set columncount = 0 %}
							{% for i in 1..blueprint['cols'] + 1 %}
								{% set colspan = 0 %}
								{% if blueprint['details']['rows'][x]['columns'][i] %}
									{% set column = blueprint['details']['rows'][x]['columns'][i] %}
									{% set colspan = column['col-length'] %}
									<td colspan="{{ colspan }}" class="{{ module_formatter.get_justifyclass(column['data-justify']) }}">
										{{ module_formatter.format_celldata(line, column) }} &nbsp;
									</td>
								{% else %}
									{% if columncount < blueprint['cols'] %}
										{% set colspan = 1 %}
										<td></td>
									{% endif %}
								{% endif %}
								{% set columncount = columncount + colspan %}
							{% endfor %}
						</tr>
					{% endfor %}
				{% endfor %}

				{% if blueprint['totals'] and quote['totals'] %}
					{% for x in 1..blueprint['totals']['rowcount']  %}
						<tr>
							{% set columncount = 0 %}
							{% for i in 1..blueprint['cols'] + 1 %}
								{% set colspan = 0 %}
								{% if blueprint['totals']['rows'][x]['columns'][i] %}
									{% set column = blueprint['totals']['rows'][x]['columns'][i] %}
									{% set colspan = column['col-length'] %}
									<td colspan="{{ colspan }}" class="{{ module_formatter.get_justifyclass(column['data-justify']) }}">
										<span class="font-weight-bold">{{ column['label'] }}</span>: {{ module_formatter.format_celldata(quote['totals'], column) }} &nbsp;
									</td>
								{% else %}
									{% if columncount < blueprint['cols'] %}
										{% set colspan = 1 %}
										<td></td>
									{% endif %}
								{% endif %}
								{% set columncount = columncount + colspan %}
							{% endfor %}
						</tr>
					{% endfor %}
				{% endif %}
			{% endfor %}
		</tbody>
	</table>
{% endfor %}
