<div class="form-group">
	<label for="contact">Contact</label>
	<input type="text" class="form-control form-control-sm" name="contact" id="contact" value="{{ po.contact }}">
</div>
<div class="form-group">
	<label for="phone">Phone</label>
	<input type="text" class="form-control form-control-sm phone-input" name="phone" id="phone" value="{{ po.phone|phone_us }}">
</div>
<div class="form-group">
	<label for="fax">Fax</label>
	<input type="text" class="form-control form-control-sm phone-input" name="fax" id="fax" value="{{ po.fax|phone_us }}">
</div>
<div class="form-group">
	<label for="email">Email</label>
	<input type="email" class="form-control form-control-sm" name="email" id="email" value="{{ po.email }}">
</div>
