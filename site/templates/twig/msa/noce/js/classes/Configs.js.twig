class NoceConfigs {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new NoceConfigs();
		}
		return this.instance;
	}

	constructor() {
		this.fields = {
			code: {
				maxlength: {{ qnotes.fieldAttribute('code', 'maxlength') }}
			},
			notes: {
				cols: {{ qnotes.fieldAttribute('note', 'cols') }}
			},
		};
	}
}
