<div class="row">
	<div class="form-group col-sm-3">
		<label for="admin">System Administrator</label>
		{% include 'util/bootstrap/form/select-simple.twig' with {
				'select': {
					'name': 'admin',
					'addclasses': ['form-control-sm'],
					'value' : duser.isAdmin() ? 'Y' : 'N',
					'haskeys': true,
					'options': {'Y': 'Yes', 'N': 'No'}
