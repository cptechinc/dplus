<div class="form-group row">
	<div class="col-sm-3">
		<label for="restrictaccess">Restrict Div Access</label>
	</div>
	<div class="col-sm-3">
		{% include 'util/bootstrap/form/select-simple.twig' with {
				'select': {
					'name': 'restrictaccess',
					'addclasses': ['form-control-sm'],
					'value' : duser.restrictAccess() ? 'Y' : 'N',
					'haskeys': true,
					'options': {'Y': 'Yes', 'N': 'No'}
				}
			}
		%}
	</div>
</div>

<div class="form-group row">
	<div class="col-sm-3">
		<label for="allowprocessdelete">Allow Process Removal</label>
	</div>
	<div class="col-sm-3">
		{% include 'util/bootstrap/form/select-simple.twig' with {
				'select': {
					'name': 'allowprocessdelete',
					'addclasses': ['form-control-sm'],
					'value' : duser.allowProcessDelete() ? 'Y' : 'N',
					'haskeys': true,
					'options': {'Y': 'Yes', 'N': 'No'}
				}
			}
		%}
	</div>
</div>
