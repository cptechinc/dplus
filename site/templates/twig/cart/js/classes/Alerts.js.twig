class CartAlerts extends Alerts {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new CartAlerts();
		}
		return this.instance;
	}

	minMarginRequirements(linenbr) {
		swal2.fire({
			icon: 'warning',
			title: 'Attention',
			text: 'Line #'+ linenbr +' does not meet minimum margin requirements, saving this item will place this order on hold',
			confirmButtonText: 'Ok'
		});
	}

}
