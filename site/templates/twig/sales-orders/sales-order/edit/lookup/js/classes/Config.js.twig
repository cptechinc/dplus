class LookupConfig {
	static instance = null;

	static getInstance() {
		if (this.instance === null) {
			this.instance = new LookupConfig();
		}
		return this.instance;
	}

	constructor() {
		this.custid = '{{ order.custid }}';
		this.shiptoid = '{{ order.shiptoid }}';
		this.fields = {
			price: {
				precision: {{ eso.decimal_places_price() }}
			},
			qty: {
				precision: {{ eso.decimal_places_qty() }}
			},
		}
	}

}
